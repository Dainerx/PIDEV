                    </div>


                    <!-- Main Navigation -->
                    <nav id="navigation" class="style-1">
                        <ul id="responsive">

                            <li><a href="{{ path('homeuser') }}">Home</a>

                            </li>

                            <li><a href="#">Listings</a>
                                <ul>
                                    <li><a href="#">List Layout</a>

                                    </li>
                                    <li><a href="#">Grid Layout</a>

                                    </li>
