                    <nav id="breadcrumbs">
                        <ul>
                            <li><a href="{{ path('homeuser') }}">Home</a></li>
                            <li>Log In & Register</li>
                        </ul>
                    </nav>

                </div>
            </div>
        </div>
    </div>


    <!-- Contact
    ================================================== -->

